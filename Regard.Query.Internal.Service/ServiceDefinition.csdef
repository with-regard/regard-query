<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Regard.Query.Internal.Service" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WebRole name="Regard.Query.BasicEndpoint" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Regard.Query.WebAPI" />
        </Bindings>
      </Site>
    </Sites>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Endpoints>
      <InternalEndpoint name="Regard.Query.WebAPI" protocol="http" port="8080" />
    </Endpoints>
  </WebRole>
</ServiceDefinition>